<%- include('partials/head') %>

<div class="container">
  <h1>Your random number is: <%= random %> </h1>
  <% if (random % 2 === 0) {%>

  <h2>That is an even number</h2>

  <% } else { %>

  <h2>That is an odd number</h2>

  <% } %>

  <h3>That number is: <%= (random % 2 === 0) ? 'EVEN' : 'ODD' %> </h3>
</div>

